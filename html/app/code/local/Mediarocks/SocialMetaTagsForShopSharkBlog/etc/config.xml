<config>

    <modules>
        <Mediarocks_SocialMetaTagsForShopSharkBlog>
            <version>0.0.1</version>
        </Mediarocks_SocialMetaTagsForShopSharkBlog>
    </modules>
	
    <admin>
        <routers>
            <blog_admin>
                <args>
                    <modules>
                        <blog_admin before="ShopShark_Blog">Mediarocks_SocialMetaTagsForShopSharkBlog</blog_admin>
                    </modules>
                </args>
            </blog_admin>
        </routers>
    </admin>
	
	
    <global>
    	
        <blocks>
            <blog>
                <rewrite>
                    <manage_blog_edit_tab_options>Mediarocks_SocialMetaTagsForShopSharkBlog_Block_Manage_Blog_Edit_Tab_Options</manage_blog_edit_tab_options>
                    <manage_cat_edit_form>Mediarocks_SocialMetaTagsForShopSharkBlog_Block_Manage_Cat_Edit_Form</manage_cat_edit_form>
                </rewrite>
            </blog>
        </blocks>
		
        <models>
            <mediarocks_socialmetatagsforshopsharkblog>
                <class>Mediarocks_SocialMetaTagsForShopSharkBlog_Model</class>
            </mediarocks_socialmetatagsforshopsharkblog>
            <mediarocks_socialmetatags>
                <rewrite>
                    <head>Mediarocks_SocialMetaTagsForShopSharkBlog_Model_Head</head>
                </rewrite>
            </mediarocks_socialmetatags>
        </models>
		
        <helpers>
            <mediarocks_socialmetatagsforshopsharkblog>
                <class>Mediarocks_SocialMetaTagsForShopSharkBlog_Helper</class>
            </mediarocks_socialmetatagsforshopsharkblog>
        </helpers>
        
        <resources>
            <mediarocks_socialmetatagsforshopsharkblog_setup>
                <setup>
                    <module>Mediarocks_SocialMetaTagsForShopSharkBlog</module>
					<class>Mediarocks_SocialMetaTagsForShopSharkBlog_Model_Resource_Mysql4_Setup</class>
                </setup>
				<connection>
					<use>core_setup</use>
				</connection>
            </mediarocks_socialmetatagsforshopsharkblog_setup>
            <mediarocks_socialmetatagsforshopsharkblog_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </mediarocks_socialmetatagsforshopsharkblog_write>
            <mediarocks_socialmetatagsforshopsharkblog_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </mediarocks_socialmetatagsforshopsharkblog_read>
        </resources>
        
    </global>
	
	
    <frontend>
    	
        <layout>
            <updates>
                <mediarocks_socialmetatagsforshopsharkblog>
                    <file>mediarocks_socialmetatagsforshopsharkblog.xml</file>
                </mediarocks_socialmetatagsforshopsharkblog>
            </updates>
        </layout>
        
    </frontend>
    
</config>
