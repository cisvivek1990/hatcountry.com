/*---------------------product, delivery time-------------------------*/
#dev-time {
    clear: both;
    margin: 5px 0;
}

#dev-time .orange {
    background:url(../images/common/delivery/0-2.png) no-repeat; 
}

#dev-time .dark-blue {
    background:url(../images/common/delivery/1-4.png) no-repeat;
}

#dev-time .light-blue {
    background:url(../images/common/delivery/4-8.png) no-repeat;
}

#dev-time .star {
    display: inline-block;
    height: 62px;
    width: 62px;
    text-align: center;
}

#dev-time .star span {
    color: #FFFFFF;
    display: block;
    font-size: 12px;
    padding-top: 14px;
    line-height: 15px;
}

#dev-time .light-blue span{
    font-size: 11px;
    padding-top: 17px;
}

#dev-time .text {
    display: inline-block;
    margin: 5px 0 0 5px;
    line-height: 12px;
}

#dev-time .text span {
    font-size: 15px;
}

#dev-time .or,
#dev-time .lb{
    vertical-align: top;
}
#dev-time .or span{
    color: #ec5355;
}
#dev-time .db span{
    color: #32669f;
}
#dev-time .lb span{
    color: #2f97cc;
}
#dev-time .text p.title {
    font-size: 12px;
    margin-top: 4px;
}

#dev-time .text p.sub {
    font-size: 10px;
    margin-top: 3px;
}

#narrow-by-list dd.odd,#narrow-by-list dd.even {
    max-height: 187px;
    overflow-y: auto;
}

.hidden {
    display: none;
}

@media (max-width: 991px) {

    .banner-left, .banner-right
    {
        display: none;
    }

    #narrow-by-list dt.odd,#narrow-by-list dt.even {
        background-size: contain;
        background: url(../../../ma_greek/ma_greek1/images/menu_down.png) no-repeat 100% 50%;
    }

    #narrow-by-list dt.odd.active,#narrow-by-list dt.even.active {
        background-size: contain;
        background: url(../../../ma_greek/ma_greek1/images/menu_up.png) no-repeat 100% 50%;
    }


    .block.block-layered-nav #narrow-by-list dt
    {
        cursor: pointer;
    }
}

